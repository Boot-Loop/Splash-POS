<Application x:Class="UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="applicationStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/src/styles/button/common_button_style/CommonButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/button/common_button_style_2/CommonButtonStyle2.xaml"/>
                <ResourceDictionary Source="/src/styles/button/warning_button_style/WarningButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/button/green_button_style/GreenButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/button/flat_button_style/FlatButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/colors/Colors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <FontFamily x:Key="PASSWORD_FONT">./res/fonts/password.ttf#Password</FontFamily>
            <FontFamily x:Key="FONT_ROBOTO_REGULAR">./res/fonts/Roboto-Regular.ttf#Roboto</FontFamily>

        </ResourceDictionary>
    </Application.Resources>
</Application>
