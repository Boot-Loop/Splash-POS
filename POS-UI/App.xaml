<Application x:Class="POS_UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:POS_UI"
             StartupUri="src/views/main_views/MainView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/src/styles/button/common_button_style/CommonButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/button/flat_button_style/FlatButtonStyle.xaml"/>
                <ResourceDictionary Source="/src/styles/colors/Colors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="PASSWORD_FONT">./res/fonts/password.ttf#Password</FontFamily>
            <FontFamily x:Key="FONT_ROBOTO_REGULAR">./res/fonts/Roboto-Regular.ttf#Roboto</FontFamily>

        </ResourceDictionary>
    </Application.Resources>
</Application>
