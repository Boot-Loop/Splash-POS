<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="COLOR_BLACK">#FF000000</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_WHITE">#FFFFFFFF</SolidColorBrush>

    <SolidColorBrush x:Key="COLOR_PRIMARY_DARKER">#FF424874</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_PRIMARY_DARK">#FFA6B1E1</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_PRIMARY_LIGHT">#FFDCD6F7</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_PRIMARY_LIGHTER">#FFF4EEFF</SolidColorBrush>

    <SolidColorBrush x:Key="COLOR_RED_DARKER">#FFA01629</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_RED_DARK">#FFd92232</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_RED_LIGHT">#FFe73040</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_RED_LIGHTER">#FFff4b5b</SolidColorBrush>


    <SolidColorBrush x:Key="COLOR_GREY_DARKER">#FF333333</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_GREY_DARK">#FF666666</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_GREY_LIGHT">#FF999999</SolidColorBrush>
    <SolidColorBrush x:Key="COLOR_GREY_LIGHTER">#FFCCCCCC</SolidColorBrush>
    
    

</ResourceDictionary>