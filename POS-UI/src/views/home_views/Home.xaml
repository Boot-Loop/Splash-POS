<Window x:Class="POS_UI.src.views.home_views.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POS_UI.src.views.home_views"
        mc:Ignorable="d"
        Title="Home" Height="450" Width="800">
    <DockPanel>
        <Grid Width="80" DockPanel.Dock="Left" Background="LightBlue">
            <StackPanel>
                <Label Content="LG" Height="80"/>
            </StackPanel>            
            
        </Grid>
        <Grid Height="80" DockPanel.Dock="Top" Background="LightBlue"/>
        <ContentControl x:Name="main_content_control"/>
    </DockPanel>
</Window>
